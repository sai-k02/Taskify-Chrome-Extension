{
  "name": "chrome-extension",
  "displayName": "Chrome extension",
  "version": "0.0.0",
  "description": "A basic Plasmo extension.",
  "author": "srilokh",
  "packageManager": "pnpm@6.11.0",
  "scripts": {
    "dev": "plasmo dev",
    "build": "plasmo build"
  },
  "dependencies": {
    "@plasmohq/storage": "0.13.1",
    "plasmo": "0.60.0",
    "react": "18.2.0",
    "react-dom": "18.2.0"
  },
  "devDependencies": {
    "@plasmohq/prettier-plugin-sort-imports": "3.5.2",
    "@types/chrome": "0.0.197",
    "@types/node": "18.8.3",
    "@types/react": "18.0.21",
    "@types/react-dom": "18.0.6",
    "prettier": "2.7.1",
    "querystring-es3": "^0.2.1",
    "typescript": "4.8.4",
    "url": "^0.11.0"
  },
  "manifest": {
    "host_permissions": [
      "https://*/*",
      "https://todoist.com/*"
    ],
    "permissions": [
      "identity",
      "storage"
    ]
  }
}
